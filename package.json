{
  "name": "hsu-scripts",
  "version": "1.0.7",
  "main": "lib/index.js",
  "module": "es/index.js",
  "bin": {
    "hsu-scripts": "bin/index.js"
  },
  "sideEffects": false,
  "files": [
    "bin",
    "es",
    "lib",
    "babel.js",
    ".babelrc",
    "eslint.js",
    "prettier.config.js"
  ],
  "scripts": {
    "build": "NODE_ENV='cjs' babel-node src/bin.js build src",
    "test:cli": "NODE_ENV='cjs' babel-node src/bin.js build src --es-dir esm --ignore '__specs__' '**/*.spec.js' '**/*.example.js' --cjs-dir 'cjs2'",
    "help": "NODE_ENV='cjs' babel-node src/bin.js",
    "test": "NODE_ENV='test' jest --coverage",
    "test:watch": "npm run test -- --watch",
    "flow": "flow",
    "flow-coverage": "NODE_ENV='cjs' babel-node src/bin.js flow --threshold 86",
    "eslint": "eslint ./",
    "format": "prettier --write '**/*.{js,json,md,css,yaml,yml}' '*.{js,json,md,css,yaml,yml}'",
    "changelog": "conventional-changelog --infile ./CHANGELOG.md --same-file --release-count 0 --output-unreleased"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chalk": "^2.4.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "1.3.0",
    "execa": "^1.0.0",
    "flow-copy-source": "^2.0.3",
    "flow-coverage-report": "^0.6.1",
    "rimraf": "^2.6.3",
    "yargs": "^13.2.1"
  },
  "devDependencies": {
    "@babel/node": "7.2.2",
    "codecov": "3.2.0",
    "conventional-changelog-cli": "2.0.12",
    "eslint": "5.14.1",
    "flow-bin": "0.93.0",
    "husky": "7.0.4",
    "jest": "24.1.0",
    "lint-staged": "8.1.4",
    "prettier": "1.16.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/*.example.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/es/"
    ],
    "resetMocks": true,
    "resetModules": true,
    "coverageThreshold": {
      "global": {
        "branches": 37,
        "functions": 66,
        "lines": 60,
        "statements": 58
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "./eslint.js"
    ]
  },
  "lint-staged": {
    "*.{js,json,md,css,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "description": "CLI toolbox for common scripts for npm libraries.",
  "author": "Michael Hsu <evenchange4@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evenchange4/hsu-scripts/issues",
    "email": "evenchange4@gmail.com"
  },
  "homepage": "https://github.com/evenchange4/hsu-scripts#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/evenchange4/hsu-scripts.git"
  }
}
